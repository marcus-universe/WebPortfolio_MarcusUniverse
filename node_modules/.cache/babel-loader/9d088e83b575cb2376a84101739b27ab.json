= payload\n    },\n    setMousePosition(state, { x, y }) {\n      state.mousePosition.x = x\n      state.mousePosition.y = y\n    },\n    setMouseSource(state, payload) {\n      state.mouseSource = payload\n    },\n    setVideo(state, payload) {\n      state.OpenVideo = payload\n    },\n    setLang(state, payload) {\n      state.lang = payload\n    },\n    selectSoftwareStack(state, payload) {\n      state.currentSelectedSoftwareStack = payload\n    },\n    setActiveSoftwareStack(state, { active, software, index }) {\n      if (state.softwareStack.filterKathegorie[index].active === true) {\n            console.log('already active' + active + ' ' + software + ' ' + index)\n            state.softwareStack.filterKathegorie[index].active = false\n            state.currentSelectedSoftwareStack = 'all'\n      } else {\n          state.softwareStack.filterKathegorie.forEach((item) => {\n            item.active = false\n          })\n            console.log('not active' + active + ' ' + software + ' ' + index)\n            state.softwareStack.filterKathegorie[index].active = true \n            state.currentSelectedSoftwareStack = software\n          }\n        }\n  },\n  actions: {\n    changeScreen({ commit }, payload) {\n      commit('setScreen', payload)\n    },\n    changeMousePosition({ commit }, { x, y }) {\n      commit('setMousePosition', { x, y })\n    },\n    changeMouseSource({ commit }, payload) {\n      commit('setMouseSource', payload)\n    }\n  },\n  modules: {\n  }\n})\n"],"mappings":"AAAA,SAASA,WAAW,QAAQ,MAAM;AAClC,OAAOC,KAAK,MAAM,0BAA0B;AAC5C,OAAOC,aAAa,MAAM,kCAAkC;AAE5D,eAAeF,WAAW,CAAC;EACzBG,KAAK,EAAE;IACLC,KAAK,EAAE,KAAK;IACZC,OAAO,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;IAC1CC,QAAQ,EAAE,CAAC,YAAY,EAAE,YAAY,EAAE,YAAY,CAAC;IACpDC,MAAM,EAAE,SAAS;IACjBC,IAAI,EAAE,IAAI;IACVC,aAAa,EAAE;MACbC,CAAC,EAAE,CAAC;MACJC,CAAC,EAAE;IACL,CAAC;IACDC,WAAW,EAAE,OAAO;IACpBC,SAAS,EAAE,KAAK;IAChBC,UAAU,EAAE;MACRC,OAAO,EAAE,CACD;QACQC,IAAI,EAAE,MAAM;QACZC,IAAI,EAAE,MAAM;QACZC