r\n                    </div>\r\n\r\n                </div>\r\n                \r\n            </transition>\r\n\r\n            \r\n        </template>        \r\n    </div>\r\n</template>\r\n<script>\r\nimport Logo from '@/components/Ui/Logo.vue'\r\nexport default {\r\n    name: 'ExploreBanner',\r\n    components: {\r\n        Logo\r\n    },\r\n    data() {\r\n        return {\r\n            currentIndex: 2,\r\n            slide: 'slideL'\r\n        }\r\n    },\r\n    methods: {\r\n        OpenProject() {\r\n            window.open(this.Banner[this.currentIndex].link, '_blank').focus();\r\n        },\r\n        BannerLength() {\r\n            return this.Banner.length\r\n        },\r\n        selectProject(index) {\r\n            this.currentIndex = index\r\n        },\r\n        changeindexUp() {\r\n            this.slide = 'slideR'\r\n            if (this.currentIndex < this.BannerLength() - 1) {\r\n                this.currentIndex++\r\n                \r\n            } else {\r\n                this.currentIndex = 0\r\n            }\r\n        },\r\n        changeindexDown() {\r\n            this.slide = 'slideL'\r\n            if (this.currentIndex > 0) {\r\n                this.currentIndex--\r\n                \r\n            } else {\r\n                this.currentIndex = this.BannerLength() - 1\r\n            }\r\n        },\r\n    },\r\n    computed: {\r\n        Banner() {\r\n            return this.$store.state.Banner;\r\n        }\r\n    },\r\n    \r\n}\r\n</script>"]}]}