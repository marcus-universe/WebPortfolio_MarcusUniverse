        type: Boolean,\n            required: true\n        },\n        hideHeadline: {\n            type: Boolean,\n            required: true\n        }\n    },\n    methods: {\n        hoverPlay: function () {\n            this.$refs['playButton'].setDirection(\"forward\");\n            this.$refs['playButton'].play();\n        },\n        leavePlay: function () {\n            this.$refs['playButton'].setDirection(\"reverse\");\n            this.$refs['playButton'].play();\n        },\n        OpenVideoBox: function () {\n            this.OpenVideo = true;\n        }\n    },\n    components: {\n        Vue3Lottie,\n        Showreel\n    },\n\n}\n</script>\n\n<style lang=\"scss\">\n@import '../sass/pages/startpage.sass';\n</style>\n"],"sourceRoot":""}]}