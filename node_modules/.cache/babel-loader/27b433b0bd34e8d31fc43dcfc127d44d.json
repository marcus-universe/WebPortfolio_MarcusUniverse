lass=\"HeadlineBox\">\n        <h1 class=\"Headline\">I think\n            <br>\n            <b :class=\"{smallerHeadline: smallHeadline, hideHeadline: hideHeadline}\">{{title}}</b>\n        </h1>\n        <div class=\"playcontainer\">\n\n            <Vue3Lottie\n                class=\"playbutton\"\n                ref=\"playButton\"\n                :animationData=\"require('../assets/lotties/play.json')\"\n                :loop=\"false\"\n                :autoPlay=\"false\"\n                :speed=\"1\"\n                :pauseAnimation=\"true\"\n                direction=\"reverse\"\n                v-on:mouseover=\"hoverPlay\"\n                v-on:mouseleave=\"leavePlay\" \n                @click=\"OpenVideoBox\"\n                />\n                \n\n        </div>\n\n    </div>\n\n\n<Showreel class=\"Showreelbox\" :class=\"{activeShowreel: OpenVideo}\" @click=\"OpenVideo = false\"/>\n    <!-- <div class=\"ContentBox\"></div> -->\n\n</section>\n</template>\n\n<script>\nimport Vue3Lottie from 'vue3-lottie';\nimport Showreel from './Popups/Showreel.vue';\n\nexport default {\n    name: 'StartPage',\n    data() {\n        return {\n            OpenVideo: false,\n        }\n    },\n    props: {\n        title: {\n            type: String,\n            required: true\n        },\n        smallHeadline: {\n            type: Boolean,\n            required: true\n        },\n        hideHeadline: {\n            type: Boolean,\n            required: true\n        }\n    },\n    methods: {\n        hoverPlay: function () {\n            this.$refs['playButton'].setDirection(\"forward\");\n            this.$refs['playButton'].play();\n        },\n        leavePlay: function () {\n            this.$refs['playButton'].setDirection(\"reverse\");\n            this.$refs['playButton'].play();\n        },\n        OpenVideoBox: function () {\n            this.OpenVideo = true;\n        }\n    },\n    components: {\n        Vue3Lottie,\n        Showreel\n    },\n\n}\n</script>\